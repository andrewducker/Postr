	<servlet-name>PersonaLogout</servlet-name>
	<servlet-class>com.postr.PersonaLogoutServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>PersonaLogout</servlet-name>
	<url-pattern>/personalogout</url-pattern>
